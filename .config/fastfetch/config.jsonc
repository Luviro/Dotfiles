{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "color": {
            "1": "magenta"
        },
        "source": "$HOME/.config/fastfetch/ascii/arch.txt",
        "padding": {
            "left": 4,
            "top": 2
        }
    },
    "display": {
        "separator": " : "
    },
    "modules": [
        "break",
        "break",
        {
            "type": "title",
            "color": {
                "user":"green",
                "host":"green"
            }
        },
        {
            "type": "custom",
            "format": "┌────────────────────────────────────────────┐",
            "outputColor": "magenta"
        },
        {
            "type": "os",
            "key": "  󰣇 OS       ",
            "keyColor": "green"
        },
        {
            "type": "kernel",
            "key": "   Kernel   ",
            "keyColor": "green"
        },
        {
            "type": "wm",
            "key": "   WM       ",
            "keyColor": "green"
        },
        {
            "type": "terminal",
            "key": "   Terminal ",
            "keyColor": "green"
        },
        {
            "type": "packages",
            "key": "  󰏗 Packages ",
            "keyColor": "green"
        },
        {
            "type": "display",
            "key": "  󰍹 Display  ",
            "format": "{1}x{2} @ {3}Hz [{7}]",
            "keyColor": "green"
        },
        {
            "type": "uptime",
            "key": "   Uptime   ",
            "keyColor": "green"
        },
        {
            "type": "custom",
            "format": "└────────────────────────────────────────────┘"
        }
    ]
}
